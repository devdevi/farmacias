{
    "name": "Farmacias de turno",
    "version": 1,
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(js|css|img)/.*",
        "headers": { "cache-control": "max-age=31536000, immutable" }
      },
      { "handle": "filesystem" },
      { "src": ".*", "dest": "/" }
    ],
    "alias": "farmacias.com"
  }
